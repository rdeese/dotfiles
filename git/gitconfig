[alias]
  s = status -s
  l = log --graph
  co = checkout
  cm = commit -m
  d = diff
  ds = diff --staged
  tackon = commit --amend --no-edit
  p = push
  puo = push -u origin




[format]
	pretty = %Cred%h %Cblue%ad %<(20)%Cgreen%cn %Creset%s

[log]
  abbrevCommit = true
  date = format:%F %R

[status]
  short = true

[pull]
	ff = only
